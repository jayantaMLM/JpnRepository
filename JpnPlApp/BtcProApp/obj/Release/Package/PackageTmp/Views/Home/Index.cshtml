<!--Core CSS -->
<link href="~/bs3/css/bootstrap.min.css" rel="stylesheet">
<link href="~/js/jquery-ui/jquery-ui-1.10.1.custom.min.css" rel="stylesheet">
<link href="~/css/bootstrap-reset.css" rel="stylesheet">
<link href="~/font-awesome/css/font-awesome.css" rel="stylesheet">
<link href="~/js/jvector-map/jquery-jvectormap-1.2.2.css" rel="stylesheet">
<link href="~/css/clndr.css" rel="stylesheet">
<!--clock css-->
<link href="~/js/css3clock/css/style.css" rel="stylesheet">
<!--Morris Chart CSS -->
<link rel="stylesheet" href="~/js/morris-chart/morris.css">
<!-- Custom styles for this template -->
<link href="~/css/style.css" rel="stylesheet">
<link href="~/css/style-responsive.css" rel="stylesheet" />

<script src="~/Scripts/angular/angular.min.js"></script>
<script src="~/app/scripts/DashboardController.js"></script>

<section class="wrapper">
    <div ng-app="app" ng-controller="Dashboard" ng-cloak>
        <div class="col-md-8">
            <div class="row">
                <div class="col-sm-12">
                    <section class="panel">
                        <header class="panel-heading">
                            <i class="fa fa-user" style="color:green"></i> Profile Detail
                        </header>
                    </section>
                </div>
            </div>
            <div class="row">
                <div class="panel" style="margin-left:15px;margin-right:15px;margin-top:-22px">
                    <div class="row">
                        <div class="col-md-4" style="border:black;text-align:center">
                            <img src="~/Content/avatar1.jpg" style="height:30%;width:30%;padding-top:80px;margin-left:20px" />
                            <h3>{{Name}}</h3>
                        </div>
                        <div class="col-md-8">
                            <table style="width:100%;line-height:50px;">
                                <tr>
                                    <td style="text-align:right;padding-right:10px;background-color:#d4dfdf">Package.</td>
                                    <td style="width:55%;padding-left:10px;border-bottom:1px solid lightgrey">{{package}}</td>
                                </tr>
                                <tr>
                                    <td style="text-align:right;padding-right:10px;background-color:#d4dfdf">Mobile No.</td>
                                    <td style="padding-left:10px;border-bottom:1px solid lightgrey">{{MobileNo}}</td>
                                </tr>
                                <tr>
                                    <td style="text-align:right;padding-right:10px;background-color:#d4dfdf">Email Id.</td>
                                    <td style="padding-left:10px;border-bottom:1px solid lightgrey">{{Emailid}}</td>
                                </tr>
                                <tr>
                                    <td style="text-align:right;padding-right:10px;background-color:#d4dfdf">Signup Date.</td>
                                    <td style="padding-left:10px;border-bottom:1px solid lightgrey">{{SignupDate}}</td>
                                </tr>
                                <tr>
                                    <td  style="text-align:right;padding-right:10px;background-color:#d4dfdf">Royalty Achievement.</td>
                                    <td style="padding-left:10px;border-bottom:1px solid lightgrey">{{RoyaltyClub}}</td>
                                </tr>
                                <tr>
                                    <td  style="text-align:right;padding-right:10px;background-color:#d4dfdf">Diamond Royalty Achievement.</td>
                                    <td style="padding-left:10px;border-bottom:1px solid lightgrey">{{DiamondRoyaltyClub}}</td>
                                </tr>
                            </table>

                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-md-4" ng-click="CashWallet();">
                    <div class="mini-stat clearfix" style="cursor:pointer">
                        <span class="mini-stat-icon orange"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{DevelopmentBalance | currency:'BDT ' }}</span>
                            <p>Development Wallet Balance</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" ng-click="ReserveWallet();">
                    <div class="mini-stat clearfix" style="cursor:pointer">
                        <span class="mini-stat-icon tar"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{ReferralBalance | currency:'BDT ' }}</span>
                            <p>Referral Wallet Balance</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" ng-click="ReturnWallet();">
                    <div class="mini-stat clearfix" style="cursor:pointer">
                        <span class="mini-stat-icon pink"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{RoyaltyBalance | currency:'BDT ' }}</span>
                            <p>Royalty Wallet Balance</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="mini-stat clearfix" style="cursor:not-allowed">
                        <span class="mini-stat-icon green"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{DiamondRoyaltyBalance | currency:'BDT ' }}</span>
                            <p>Diamond Royalty Wallet Balance</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="mini-stat clearfix" style="cursor:not-allowed">
                        <span class="mini-stat-icon green"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{JoiningBalance | currency:'BDT ' }}</span>
                            <p>Joining Wallet Balance</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="mini-stat clearfix" style="cursor:not-allowed">
                        <span class="mini-stat-icon green"><i class="fa fa-money"></i></span>
                        <div class="mini-stat-info">
                            <span>{{WithdrawBalance | currency:'BDT ' }}</span>
                            <p>Withdraw Wallet Balance</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4" >
            <div class="row">
                <div class="col-sm-12">
                    <section class="panel">
                        <header class="panel-heading">
                            <i class="fa fa-user" style="color:green"></i> JPN News
                        </header>
                        <div class="panel-body" style="background-color:white;min-height:547px;vertical-align:middle">
                            <h4>Welcome to JPNPL!!!</h4>
                        </div>
                    </section>
                </div>
            </div>

        </div>
    </div>
</section>


