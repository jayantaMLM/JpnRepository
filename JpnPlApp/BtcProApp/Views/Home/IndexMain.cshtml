    <script type="text/javascript">
        function showRegistrationPage() {
            $("#mu-slider").hide();
            $("#mu-services").hide();
            $("#mu-about-us").hide();
            $("#mu-portfolio").hide();
            $("#mu-certificate").hide();
            $("#registrationPage").show();
            $("#loginPage").hide();
            debugger;
            var sel = 'div[ng-controller="Register"]';
            var $scope = angular.element(sel).scope();
            $scope.isComplete = false;
            $scope.isRegistered = false;
            $scope.registerModel.ReferrerName = "";
            $scope.registerModel.FullName = "";
            $scope.registerModel.EmailId = "";
            $scope.registerModel.UserName = "";
            $scope.registerModel.Password = "";
            $scope.registerModel.ConfirmPassword = "";
            $scope.$apply();
            $("#registrationPage").show();
        }
        function showLoginPage() {
            $("#mu-slider").hide();
            $("#mu-services").hide();
            $("#mu-about-us").hide();
            $("#mu-portfolio").hide();
            $("#mu-certificate").hide();
            $("#registrationPage").hide();
            $("#loginPage").show();
        }
        function otherClick() {
            $("#mu-slider").show();
            $("#mu-services").show();
            $("#mu-about-us").show();
            $("#mu-portfolio").show();
            $("#mu-certificate").show();
            $("#loginPage").hide();
            $("#registrationPage").hide();
        }
