        <div id="loginPage" style="display:none">
            <div class="login-body" style="background:#f5f5f5">
                <div class="container">
                    <form name="Login" class="form-signin" style="height:340px;margin-top:200px;margin-bottom:100px">

                        <div class="login-wrap" style="width:400px;padding-bottom:0px;padding-top:0px;margin:auto !important;overflow:hidden auto">
                            <div class="user-login-info" style="margin-bottom:0px">
                                <div class="col-md-12">
                                    <h3 class="form-signin-heading" style="margin-bottom:25px;margin-left:115px">Member Login</h3>
                                </div>
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Username</label>
                                        <input name="Username0" ng-model="loginModel.UserName" ng-blur="checkLoginUsername()" ng-pattern="/^[A-Za-z0-9]*$/" ng-minlength="6" ng-maxlength="30" ng-required="true" />
                                        <div class="errorcls">{{loginErrModel.UserName}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Password</label>
                                        <input type="password" name="Password0" ng-model="loginModel.Password" ng-pattern="/^[A-Za-z0-9]*$/" ng-minlength="6" ng-maxlength="14" ng-required="true" />
                                        <div class="errorcls">{{loginErrModel.Password}}</div>
                                    </md-input-container>
                                </div>
                            </div>
                            <div style="text-align:center">
                                <a href="#" id="resetlink" style="color:#3b86f5;margin-left:25px;padding:3px">Forgot Password?</a>
                                <input type="button" value="Login" class="btn btn-info" ng-click="sendForLogin()" ng-disabled="Login.$invalid || loginErrModel.UserName!=''" style="height:35px;width:30%;margin-bottom:30px;" />
                            </div>
                        </div>
                    </form>
                </div>
            </div>

        </div>

        <div id="registrationPage" style="display:none">
            <div class="login-body" style="background:#f5f5f5">
                <div class="container">
                    <form name="Registration" class="form-signin" style="height:625px;margin-top:95px;margin-bottom:100px">
                        <h2 class="form-signin-heading">register now</h2>
                        <div class="login-wrap" style="padding-bottom:0px;padding-top:0px;overflow:hidden auto">
                            <div class="user-login-info" ng-show="!isComplete && !isRegistered" style="margin-bottom:0px">
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Sponsor name</label>
                                        <input name="ReferrerName" ng-model="registerModel.ReferrerName" ng-change="validation1()" ng-blur="checkReferrername()" ng-required="true" />
                                        <div class="errorcls">{{errorModel.ReferrerName}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Member name</label>
                                        <input name="Fullname" ng-model="registerModel.FullName" ng-required="true" ng-change="validation2()" />  @*ng-pattern="/^[a-zA-Z ]*$/"*@
                                        <div class="errorcls">{{errorModel.FullName}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Email Id</label>
                                        <input type="email" name="Email" ng-model="registerModel.EmailId" ng-required="true" ng-change="validation3()" ng-blur="checkEmailId()" />
                                        <div class="errorcls">{{errorModel.EmailId}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-12">
                                    <md-input-container style="width:100%;margin:10px">
                                        <label>Unique Username</label>
                                        <input name="Username" ng-model="registerModel.UserName" ng-keyup="validation4()" ng-blur="checkUsername()" ng-pattern="/^[A-Za-z0-9]*$/" ng-minlength="6" ng-maxlength="30" ng-required="true" />
                                        <div class="errorcls">{{errorModel.UserName}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-6">
                                    <md-input-container style="margin:10px">
                                        <label>Password</label>
                                        <input type="password" name="Password" ng-model="registerModel.Password" ng-change="validation5()" ng-pattern="/^[A-Za-z0-9]*$/" ng-minlength="6" ng-maxlength="14" ng-required="true" />
                                        <div class="errorcls">{{errorModel.Password}}</div>
                                    </md-input-container>
                                </div>
                                <div class="col-md-6">
                                    <md-input-container style="margin:10px">
                                        <label>ReType Password</label>
                                        <input type="password" name="Password2" ng-model="registerModel.ConfirmPassword" ng-change="validation6()" ng-required="true" />
                                        <div class="errorcls">{{errorModel.ConfirmPassword}}</div>
                                    </md-input-container>
                                </div>

                                <div style="margin-left:28px;margin-right:10px">
                                    <p style="margin-bottom:10px;font-weight:700;color:rgba(0,0,0,0.54);font-size:xx-small">Position</p>
                                    <input type="radio" ng-model="registerModel.BinaryPosition" value="L" name="BinaryPos" checked="checked" />Left &nbsp;&nbsp;&nbsp; <input type="radio" ng-model="registerModel.BinaryPosition" value="R" name="BinaryPos" />Right
                                    <hr style="margin-top:10px;margin-bottom:0px;border-top-color:#d4d2d2" />
                                </div>
                            </div>
                        </div>

                        <div style="text-align:center">
                            <input type="button" value="REGISTER" class="btn btn-info" ng-disabled="Registration.$invalid || errorModel.ReferrerName!='' || errorModel.UserName!=''" ng-click="showSubmit=true;register();registration()" style="height:35px;width:30%;margin-bottom:30px;" ng-show="!isComplete && !isRegistered" />
                        </div>
                    </form>
                </div>
            </div>
